<?xml version="1.0" encoding="UTF-8" ?>

<!-- Overhead Panel -->

<PropertyList>

  <path>oh-panel.ac</path>

  <!-- Batt Switch Cover -->
  <animation>
    <type>rotate</type>
    <object-name>BattSwitchCover</object-name>
    <property>/controls/electric/battswitchcover</property>
    <factor>-80</factor>
    <axis>
      <x> 0</x>
      <y> 1</y>
      <z> 0</z>
    </axis>
    <center>
      <x-m> -0.00299</x-m>
      <y-m> -0.46566</y-m>
      <z-m> 0.49062</z-m>
    </center>  
  </animation>

  <animation>
    <type>pick</type>
    <object-name>BattSwitchCover</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/electric/battswitchcover</property>
      </binding>
    </action>
  </animation>

  <!-- Batt Switch -->

  <animation>
    <type>rotate</type>
    <object-name>BattSwitch</object-name>
    <property>/controls/electric/battery-switch</property>
    <factor>40</factor>
    <axis>
      <x> 0</x>
      <y> 1</y>
      <z> 0</z>
    </axis>
    <center>
      <x-m> -0.00189</x-m>
      <y-m> -0.46451</y-m>
      <z-m> 0.47184</z-m>
    </center>  
  </animation>

  <animation>
    <type>pick</type>
    <object-name>BattSwitch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/electric/battery-switch</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>
          electrical.set_elec()
        </script>
      </binding>
    </action>
  </animation>

  <!-- Bus Tran Switch Cover -->

  <animation>
    <type>rotate</type>
