    <object-name>ExitLightSwitch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/electric/exitlightswitch</property>
      </binding>
    </action>
  </animation>


  <animation>
    <type>material</type>
    <object-name>grdpwrindlight</object-name>
    <ambient>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </ambient>
    <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
    <condition>
      <property>/controls/groundservice/gpu</property>
    </condition>
  </animation>

  <animation>
    <type>material</type>
    <object-name>grdpwrindlight</object-name>
    <ambient>
      <red>   0.1 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </ambient>
    <emission>
      <red>   0.1 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </emission>
    <condition>
      <not>
        <property>/controls/groundservice/gpu</property>
      </not>
    </condition>
  </animation>


</PropertyList>
