<!--No Smoking-->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>nosmoking</object-name>
    <action>
      <name>No Smoking Switch</name>
      <button>0</button>
      <repeatable>false</repeatable> 
      <binding>
