<!-- Captains FD Switch -->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>cap-fd</object-name>
    <action>
      <name>Captains FD switch</name>
      <button>0</button>
      <repeatable>false</repeatable> 
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/flightdirector/fd-on</property>
        <step>1</step>
        <min>0></min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

