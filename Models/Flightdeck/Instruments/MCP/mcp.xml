<!-- =============================================================  -->
<!-- SPEED CONTROL                                                 -->
<!-- =============================================================  --> 


<!-- ATArm Switch -->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>ATArm</object-name>
    <action>
      <name>AT Arm Switch</name>
      <button>0</button>
      <repeatable>false</repeatable> 
      <binding>
        <command>property-toggle</command>
        <property>/autopilot/internal/SPD</property>
        <step>1</step>
        <min>0></min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ATArm</object-name>
    <property>/autopilot/internal/SPD</property>
    <factor>-30</factor>
    <offset-deg>30</offset-deg>
    <center>
      <x-m>-0.00019</x-m>
      <z-m>0.05404</z-m>
    </center>
    <axis>
      <y>1</y>
    </axis>
  </animation>

<!-- MACH Sel -->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>IAS-SEL</object-name>
    <action>
      <name>MACH Sel Switch</name>
      <button>0</button>
      <repeatable>false</repeatable> 
      <binding>
        <command>property-toggle</command>
        <property>/autopilot/internal/SPD-IAS</property>
        <step>1</step>
        <min>0></min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>/autopilot/internal/SPD-MACH</property>
        <step>1</step>
        <min>0></min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IAS-SEL</object-name>
    <property>/autopilot/internal/SPD-MACH</property>
    <factor>-30</factor>
    <offset-deg>30</offset-deg>
    <center>
      <x-m>-0.00019</x-m>
      <z-m>0.04060</z-m>
    </center>
    <axis>
      <y>1</y>
    </axis>
  </animation>

<!-- Speed Decrease --> 

  <animation>
    <type>pick</type>
    <visible>false</visible>
    <object-name>speeddecrease</object-name>
    <action>
      <name>airspeed click (decrease)</name>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>-1.0</step>
        <min>0</min>   
        <max>350</max>    
      </binding>
    </action>
    <action>
      <name>airspeed scroll (decrease)</name>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>-10.0</step>
        <min>0</min>   
        <max>350</max>    
      </binding>
    </action>
    <action>
      <name>airspeed scroll (increase)</name>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>10.0</step>
        <min>0</min>   
        <max>350</max>    
      </binding>
    </action>
  </animation>

